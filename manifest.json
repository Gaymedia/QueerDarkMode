{
  "manifest_version": 2,
 
  "name": "QueerDarkMode",
  "version": "0.1.2",
  "description": "Dark design for the social network DBNA",
  
  "icons": { "128": "icon.png", "256": "icon256.png" },
 
  "content_scripts": [{
    "run_at": "document_end",
    "js": ["js/modify_dark.js"],
    "matches": ["https://www.dbna.com/*"]
  }],

  "web_accessible_resources": [
    "css/*.css"
  ],
  
  "permissions": []
}